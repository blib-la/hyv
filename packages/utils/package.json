{
  "name": "@hyv/utils",
  "version": "0.6.0",
  "license": "AGPL",
  "type": "module",
  "homepage": "https://github.com/failfa-st/hyv/",
  "bugs": {
    "url": "https://github.com/failfa-st/hyv/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/failfa-st/hyv.git"
  },
  "author": {
    "name": "Gregor Adams",
    "email": "greg@pixelass.com"
  },
  "keywords": [
    "ai",
    "agents",
    "artificial-intelligence",
    "openi",
    "gpt",
    "llama",
    "alpacca"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./url": {
      "types": "./dist/url.d.ts",
      "default": "./dist/url.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "default": "./dist/string.js"
    },
    "./side-effects": {
      "types": "./dist/side-effects.d.ts",
      "default": "./dist/side-effects.js"
    },
    "./fs": {
      "types": "./dist/fs.d.ts",
      "default": "./dist/fs.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    },
    "./parsers": {
      "types": "./dist/parsers/index.d.ts",
      "default": "./dist/parsers/index.js"
    },
    "./parsers/code": {
      "types": "./dist/parsers/code.d.ts",
      "default": "./dist/parsers/code.js"
    },
    "./parsers/markdown": {
      "types": "./dist/parsers/markdown.d.ts",
      "default": "./dist/parsers/markdown.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "clean": "npx rimraf dist"
  },
  "dependencies": {
    "camelcase": "7.0.1",
    "humanize-string": "3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
